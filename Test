local function rot13(str)
 return (str:gsub("%a",function(c)
  local b=c:byte()
  if b>=65 and b<=90 then return string.char(((b-65+13)%26)+65)
  elseif b>=97 and b<=122 then return string.char(((b-97+13)%26)+97)
  else return c end
 end))
end

-- Obfuscated ROT13 wrapper
local function a4(str)
 local x=""
 for i=1,#str do
  local c=str:sub(i,i)
  local n=c:byte()
  if (n>=65 and n<=90)then n=((n-65+13)%26)+65
  elseif(n>=97 and n<=122)then n=((n-97+13)%26)+97 end
  x=x..string.char(n)
 end
 return x
end

