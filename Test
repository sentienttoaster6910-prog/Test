label.Position=UDim2.new(0,0,1,-30)
label.BackgroundTransparency=1
label.TextColor3=Color3.new(1,1,1)
label.Font=Enum.Font.SourceSansBold
label.TextScaled=true
label.Text=a4("ZNQR OL PVA, ZpAbboFgner ba ebolok")
label.Parent=gui

local dashBtn=Instance.new("TextButton")
dashBtn.Size=UDim2.new(0,130,0,45)
dashBtn.Position=UDim2.new(0,20,0,20)
dashBtn.BackgroundColor3=Color3.fromRGB(0,128,128)
dashBtn.TextColor3=Color3.new(1,1,1)
dashBtn.Font=Enum.Font.SourceSansBold
dashBtn.TextScaled=true
