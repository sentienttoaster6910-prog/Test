local P=game:GetService("Players")local R=game:GetService("RunService")local T=game:GetService("TweenService")local L=P.LocalPlayer local C=L.Character or L.CharacterAdded:Wait() local H=C:WaitForChild("HumanoidRootPart") local M=C:WaitForChild("Humanoid") local S=Instance.new("Part")S.Size=Vector3.new(5,3,0.3)S.Anchored=true S.CanCollide=false S.Transparency=0.6 S.BrickColor=BrickColor.new("Really black") S.Name="HackScreenPart" S.Parent=workspace local G=Instance.new("SurfaceGui")G.Face=Enum.NormalId.Front G.AlwaysOnTop=true G.PixelsPerStud=50 G.Parent=S local N=Instance.new("TextLabel")N.Size=UDim2.new(1,0,1,0)N.BackgroundTransparency=1 N.TextColor3=Color3.new(1,1,1) N.Font=Enum.Font.SourceSansBold N.TextScaled=true N.TextWrapped=true N.Text="Controls:\n- Dash (Button)\n- Taunt (Button)" N.Parent=G local B=Instance.new("BillboardGui") B.Adornee=H B.Size=UDim2.new(0,120,0,60) B.StudsOffset=Vector3.new(0,3,0) B.AlwaysOnTop=true B.Parent=H local D=Instance.new("TextLabel") D.Size=UDim2.new(1,0,1,0) D.BackgroundTransparency=1 D.Font=Enum.Font.GothamBold D.TextScaled=true D.Text="GOD" D.TextColor3=Color3.new(1,1,1) D.TextStrokeColor3=Color3.new(1,0.85,0) D.TextStrokeTransparency=0 D.Parent=B local X={"@","#","$","%","&","*","!","?","~","+","-","=","/"} local function Y(Z)local a="" for i=1,Z do a=a..X[math.random(1,#X)]end return a end local function U() local b={"local user='"..L.Name.."'", "function hack()"," for i=1,10 do"," print('Access granted')"," end","end","error('Unexpected token')","print('User verified:"..L.Name.."')","-- corrupted code block"} local c={} for i=1,5 do table.insert(c,b[math.random(1,#b)])end return c end local O={Vector3.new(5,4,5),Vector3.new(-6,3,4),Vector3.new(3,5,7),Vector3.new(-5,3,-5)} local F={} for i,v in ipairs(O)do local p=Instance.new("Part") p.Size=Vector3.new(3,2,0.2) p.Anchored=true p.CanCollide=false p.Transparency=0.5 p.BrickColor=BrickColor.new("Really black") p.Name="CodeFrame"..i p.Parent=workspace local sg=Instance.new("SurfaceGui") sg.Face=Enum.NormalId.Front sg.AlwaysOnTop=true sg.PixelsPerStud=50 sg.Adornee=p sg.Parent=p local lbl=Instance.new("TextLabel") lbl.Size=UDim2.new(1,0,1,0) lbl.BackgroundTransparency=1 lbl.TextColor3=Color3.new(1,1,1) lbl.Font=Enum.Font.Code lbl.TextScaled=true lbl.TextWrapped=true lbl.Text=table.concat(U(),"\n") lbl.Parent=sg table.insert(F,{part=p,offset=v}) end local V={} for i,d in ipairs(F)do V[i]=H.Position+ d.offset d.part.CFrame=CFrame.new(V[i],H.Position)end R.RenderStepped:Connect(function(dt) local pos=H.Position for i,d in ipairs(F)do local tgt=pos+d.offset V[i]=V[i]:Lerp(tgt,5*dt) d.part.CFrame=CFrame.new(V[i],pos)end local t=pos+H.CFrame.LookVector*-4+H.CFrame.RightVector*3+Vector3.new(0,2,0) S.CFrame=S.CFrame:Lerp(CFrame.new(t,pos),5*dt) local s=tick() local a=s*math.pi*4 D.Position=UDim2.new(0,math.cos(a)*7,0,math.sin(a)*7) D.Rotation=math.sin(s*6)*10 D.Size=UDim2.new(1+math.sin(s*8)*0.2,0,1+math.cos(s*7)*0.2,0) if math.random()>0.5 then D.Text="GOD" D.TextColor3=Color3.new(1,1,1) else D.Text=Y(3) D.TextColor3=Color3.new(math.random(),math.random(),math.random()) end end)

local function ROT13(str)local res="" for i=1,#str do local c=str:sub(i,i) local n=c:byte() if n>=97 and n<=122 then n=((n-97+13)%26)+97 elseif n>=65 and n<=90 then n=((n-65+13)%26)+65 end res=res..string.char(n) end return res end

local GUI=Instance.new("ScreenGui") GUI.Name="HackControls" GUI.Parent=L:WaitForChild("PlayerGui") local lbl2=Instance.new("TextLabel") lbl2.Size=UDim2.new(1,0,0,30) lbl2.Position=UDim2.new(0,0,1,-30) lbl2.BackgroundTransparency=1 lbl2.TextColor3=Color3.new(1,1,1) lbl2.Font=Enum.Font.SourceSansBold lbl2.TextScaled=true lbl2.Text=ROT13("znqr ol pva, mcnoobstaer ba ebolok") lbl2.Parent=GUI

local dash=Instance.new("TextButton") dash.Size=UDim2.new(0,130,0,45) dash.Position=UDim2.new(0,20,0,20) dash.BackgroundColor3=Color3.fromRGB(0,128,128) dash.TextColor3=Color3.new(1,1,1) dash.Font=Enum.Font.SourceSansBold dash.TextScaled=true dash.Text="Dash" dash.Parent=GUI dash.MouseButton1Click:Connect(function() local a=Instance.new("Attachment",H) local b=Instance.new("Attachment",H) local t=Instance.new("Trail") t.Attachment0=a t.Attachment1=b t.Lifetime=0.25 t.Transparency=NumberSequence.new{NumberSequenceKeypoint.new(0,0),NumberSequenceKeypoint.new(1,1)} t.Color=ColorSequence.new(Color3.fromRGB(0,255,255)) t.Parent=H local f=H.CFrame.LookVector local s=H.Position local e=s+f*20 local tw=T:Create(H,TweenInfo.new(0.3,Enum.EasingStyle.Quad,Enum.EasingDirection.Out),{CFrame=CFrame.new(e,e+H.CFrame.LookVector)}) tw:Play() tw.Completed:Wait() t:Destroy() a:Destroy() b:Destroy() end)

local taunt=Instance.new("TextButton") taunt.Size=UDim2.new(0,130,0,45) taunt.Position=UDim2.new(0,20,0,80) taunt.BackgroundColor3=Color3.fromRGB(128,0,128) taunt.TextColor3=Color3.new(1,1,1) taunt.Font=Enum.Font.SourceSansBold taunt.TextScaled=true taunt.Text="Taunt" taunt.Parent=GUI local snd=Instance.new("Sound") snd.SoundId="rbxassetid://17620645740" snd.Volume=1 snd.Parent=L:WaitForChild("PlayerGui") taunt.MouseButton1Click:Connect(function() snd:Play() end)

local bg=Instance.new("Sound") bg.SoundId="rbxassetid://78744747224727" bg.Volume=0.3 bg.Looped=true bg.Parent=L:WaitForChild("PlayerGui") bg:Play()
